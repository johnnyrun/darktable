msgstr "&lt;Ctrl&gt;q"

#: /home/tatica/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:17357(entry)
msgid "global/lighttable view"
msgstr "global/vista de mesa de luz"

