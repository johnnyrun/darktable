msgstr "l"

#: /home/tatica/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:17365(entry)
msgid "global/darkroom view"
msgstr "global/vista de cuarto oscuro"

#: /home/tatica/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:17368(entry)
msgid "d"
msgstr "d"

#: /home/tatica/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:17373(entry)
msgid "global/tethering view"
msgstr "global/vista de captura"

#: /home/tatica/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:17376(entry)
msgid "t"
msgstr "t"

#: /home/tatica/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:17381(entry)
